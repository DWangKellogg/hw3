<%= form_with model: @place do |form| %>
  <p>
    <%= form.label "name", "Place" %>
    <%= form.text_field "name" %>
  </p>
  <p>
    <%= form.label "created_at" %>
    <%= form.text_field "created_at" %>
    <%= form.label "updated_at" %>
    <%= form.text_field "updated_at" %>
  </p>
  <%= form.submit %>
<% end %>